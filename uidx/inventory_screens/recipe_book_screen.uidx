            {
              "gamepad_helper_right_trigger@common.gamepad_helper_right_trigger": {
                "ignored": "$ignore_next_button",
                "offset": [ 3, 4 ],
                "anchor_from": "bottom_right",
                "anchor_to": "bottom_right"
