              "gamepad_helper_left_trigger@common.gamepad_helper_left_trigger": {
                "ignored": "$ignore_prev_button",
                "offset": [ -3, 4 ],
                "anchor_from": "bottom_left",
                "anchor_to": "bottom_left"
